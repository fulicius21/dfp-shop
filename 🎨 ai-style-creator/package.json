{
  "name": "dressforp-ai-style-creator",
  "version": "1.0.0",
  "description": "DressForPleasure AI Style Creator System - KI-basierte Bildverbesserung und Content-Generierung fÃ¼r Fashion E-Commerce",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "lint": "eslint src/",
    "build": "npm run lint && npm run test",
    "docker:build": "docker build -t dressforp-ai-style-creator .",
    "docker:run": "docker run -p 8001:8001 dressforp-ai-style-creator",
    "setup": "npm install && npm run setup:models",
    "setup:models": "node scripts/setup-models.js",
    "health": "curl -f http://localhost:8001/api/health || exit 1"
  },
  "keywords": [
    "ai",
    "fashion",
    "image-processing",
    "content-generation",
    "ecommerce",
    "style-transfer",
    "huggingface",
    "transformers"
  ],
  "author": "DressForPleasure Dev Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "@xenova/transformers": "^2.8.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dressforp/ai-style-creator.git"
  },
  "bugs": {
    "url": "https://github.com/dressforp/ai-style-creator/issues"
  },
  "homepage": "https://github.com/dressforp/ai-style-creator#readme",
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "prefer-const": "error"
    }
  }
}