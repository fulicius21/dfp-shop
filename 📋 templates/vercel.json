{
  "name": "dressforp-frontend",
  "version": 2,
  "builds": [
    {
      "src": "ðŸ’» frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "framework": "vite"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "$BACKEND_URL/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/ðŸ’» frontend/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "@api_url",
    "VITE_STRIPE_PUBLISHABLE_KEY": "@stripe_key",
    "VITE_GA_TRACKING_ID": "@ga_tracking_id"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/ðŸ’» frontend/index.html"
    },
    {
      "source": "/products/(.*)",
      "destination": "/ðŸ’» frontend/index.html"
    },
    {
      "source": "/checkout/(.*)",
      "destination": "/ðŸ’» frontend/index.html"
    }
  ],
  "functions": {
    "ðŸ’» frontend/src/pages/*.tsx": {
      "maxDuration": 10
    }
  }
}
